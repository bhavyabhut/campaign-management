{
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 2,
    "overrides": [
        {
            "files": ".prettierrc",
            "options": {
                "parser": "babylon"
            }
        }
    ],
    "useTabs": false,
    "semi": true,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "proseWrap": "always"
}